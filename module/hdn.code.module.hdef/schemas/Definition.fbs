namespace Hedron.Definition;

table DefinitionMetadata
{
    version: uint;
    arch_type: string;
    type_chain: string;
}

table DefinitionSignature
{
    name: string;
}

table Definition
{
    metadata: DefinitionMetadata;
    signature: DefinitionSignature;
    dependencies: [DefinitionSignature];
    data: [ubyte];
}

root_type Definition;